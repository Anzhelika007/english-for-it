{
  "name": "english-for-it",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "preview": "vite preview",
    "prepare": "husky install",
    "storybook": "npx storybook dev -p 6006"
